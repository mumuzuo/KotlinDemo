<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_java"
    app:startDestination="@id/mainFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.zuo.demo_java.MainFragment"
        android:label="fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_msgFragment"
            app:destination="@id/msgFragment" />
    </fragment>

    <fragment
        android:id="@+id/msgFragment"
        android:name="com.zuo.demo_java.MsgFragment"
        android:label="MsgFragment"
        tools:layout="@layout/fragment_msg"/>

</navigation>